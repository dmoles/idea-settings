<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="14" />
    <option name="FONT_FAMILY" value="Hasklig" />
    <option name="LINE_SPACING" value="1.2" />
  </component>
  <component name="EditorSettings">
    <option name="IS_WHITESPACES_SHOWN" value="true" />
    <option name="SHOW_PARAMETER_NAME_HINTS" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern pattern="# \brepo\b.*" />
    <filter name="REPO">
      <pattern index="2" />
    </filter>
  </component>
</application>